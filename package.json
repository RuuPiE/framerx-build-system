    "@types/react-highlight-words": "0.11.0",
    "@types/react-modal": "3.2.1",
    "awesome-typescript-loader": "^5.2.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.4",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "backbone": "1.1.2",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.0.8",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "husky": "^0.14.3",
